<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.goodchef.liking"
          xmlns:android="http://schemas.android.com/apk/res/android"
          android:versionCode="1"
          android:versionName="1.0">

    <application
        android:name=".app.LikingApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.LikingHomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <!--团体课程详情-->
        <activity android:name=".activity.GroupLessonDetailsActivity"/>
        <!--私教课程详情-->
        <activity android:name=".activity.PrivateLessonDetailsActivity"/>
        <!--关于我们-->
        <activity android:name=".activity.AboutActivity"/>
        <!--成为教练-->
        <activity android:name=".activity.BecomeTeacherActivity"/>
        <!--周边-菜品详情-->
        <activity android:name=".activity.DishesDetailsActivity"/>
        <!--联系加盟-->
        <activity android:name=".activity.ContactJonInActivity"/>
        <!--我的信息-->
        <activity android:name=".activity.MyInfoActivity"/>
        <!-- 图片选择-->
        <activity android:name=".widgets.camera.AlbumActivity"/>
        <!--课程-->
        <activity android:name=".activity.LessonActivity"/>
        <!--用户登录-->
        <activity android:name=".activity.LoginActivity"/>
    </application>

</manifest>