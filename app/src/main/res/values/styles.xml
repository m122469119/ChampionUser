<?xml version="1.0" encoding="utf-8"?>
<resources>


    <style name="LikingItemRating" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/liking_detail_ratting_start</item>
        <item name="android:minHeight">18dip</item>
        <item name="android:maxHeight">18dip</item>
        <item name="android:isIndicator">true</item>
    </style>

    <!-- 全屏显示dialog对话框-->
    <style name="camera_dialog_no_screen">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/dialog_window_background</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <!--拍照对话框进入和退出动画样式 -->
    <style name="camera_dialog_enter_exit" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/dialog_bottom_rise_enter</item>
        <item name="android:windowExitAnimation">@anim/dialog_bottom_exit</item>
    </style>

    <style name="MyTabLayoutTextAppearance" parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textSize">@dimen/textsize_15</item>
    </style>

</resources>
